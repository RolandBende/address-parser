{
    "name": "rb/address-parser",
    "description": "A simple PHP package for extracting location-related data (street name and house number) from addresses.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rb\\AddressParser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "RolandBende",
            "email": "bende.roland@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^12.0@dev",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs --standard=PSR2 src",
        "cs-fix": "php-cs-fixer fix src --rules=@PSR2"
    }
}
